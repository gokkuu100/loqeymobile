{
  "cli": {
    "version": ">= 16.20.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://loqey.duckdns.org/api/v1",
        "EXPO_PUBLIC_WS_BASE_URL": "wss://loqey.duckdns.org/ws"
      }
    },
    "emulator": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "http://10.0.2.2:8000/api/v1",
        "EXPO_PUBLIC_WS_BASE_URL": "ws://10.0.2.2:8000/ws"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://loqey.duckdns.org/api/v1",
        "EXPO_PUBLIC_WS_BASE_URL": "wss://loqey.duckdns.org/ws"
      }
    },
    "production-apk": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://loqey.duckdns.org/api/v1",
        "EXPO_PUBLIC_WS_BASE_URL": "wss://loqey.duckdns.org/ws"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
